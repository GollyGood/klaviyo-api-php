{
    "name": "gollygood/klaviyo-api-php",
    "description": "Klaviyo email marketing service API wrapper.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "GollyGood Software",
            "email": "info@gollygoodsoftware.com"
        },
        {
            "name": "Andreas Sundqvist",
            "email": "open-source@sunkan.se"
        }
    ],
    "require": {
        "php": ">=5.5",
        "php-http/httplug": "^1.1.0",
        "psr/http-message": "^1.0",
        "psr/container": "^1.0",
        "http-interop/http-factory": "^0.3.0"
    },
    "require-dev": {
        "php-http/guzzle6-adapter": "^1.1.1",
        "phpunit/phpunit": "~5.0",
        "squizlabs/php_codesniffer": "~2.0",
        "symfony/var-dumper": "^3.1",
        "http-interop/http-factory-guzzle": "^0.3.1"
    },
    "autoload":{
        "psr-4": {
            "Klaviyo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Klaviyo\\Tests\\": "tests/"
        }
    }
